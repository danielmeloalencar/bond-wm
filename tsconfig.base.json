{
  "compilerOptions": {
    "sourceMap": true,
    "rootDir": ".",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ESNext",
    "jsx": "react",
    "noEmit": true,
    "importHelpers": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": [],
  "exclude": ["node_modules"]
}
