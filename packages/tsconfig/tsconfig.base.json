{
  "include": [],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "sourceMap": true,
    "rootDir": ".",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "useDefineForClassFields": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "ts-node": {
    "swc": true
  }
}
