{
  "name": "@electron-wm/titlebar",
  "version": "1.0.0",
  "description": "Frame window titlebar for electron-wm",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/packages/titlebar/index.d.ts",
  "repository": "https://github.com/wnayes/electron-wm",
  "author": "Will Nayes",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b ./tsconfig.json && esbuild ./index.tsx --bundle --platform=node --packages=external --loader:.svg=dataurl --outfile=./dist/index.js"
  },
  "dependencies": {
    "@electron-wm/shared": "workspace:*",
    "@electron-wm/renderer-shared": "workspace:*",
    "@electron-wm/plugin-utils": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "esbuild": "^0.16.12",
    "type-fest": "^3.3.0"
  }
}
